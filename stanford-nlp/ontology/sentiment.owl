<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
        <!ENTITY lumify "http://lumify.io#" >
        <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
        <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
        <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
        <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
        ]>

<rdf:RDF xmlns="http://lumify.io/sentiment#"
         xml:base="http://lumify.io/sentiment"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:lumify="http://lumify.io#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://lumify.io/sentiment">
        <owl:imports rdf:resource="http://lumify.io/dev"/>
    </owl:Ontology>

    <owl:DatatypeProperty rdf:about="http://lumify.io/sentiment#sentiment">
        <rdfs:label xml:lang="en">Sentiment</rdfs:label>
        <rdfs:domain rdf:resource="&owl;Thing"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>

</rdf:RDF>
